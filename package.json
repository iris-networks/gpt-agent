{
  "name": "zenobia",
  "version": "0.1.0",
  "description": "Zenobia project",
  "private": true,
  "scripts": {
    "build": "tsc && cp -r src/public dist",
    "dev": "ts-node src/main/main.ts",
    "lint": "eslint . --ext .ts",
    "test": "vitest run",
    "clean": "rm -rf dist node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start": "node dist/main/main.js"
  },
  "dependencies": {
    "@ui-tars/operator-browser": "1.2.0",
    "@ui-tars/operator-nut-js": "1.2.0",
    "@ui-tars/sdk": "1.2.0",
    "@ui-tars/shared": "1.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.14.8",
    "eslint": "^8.56.0",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "5.7.2",
    "vitest": "^1.2.1"
  },
  "packageManager": "pnpm@8.15.4"
}
