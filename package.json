{
  "name": "iris_cua",
  "version": "0.0.1",
  "description": "Iris CUA is a framework for building computer use agents!",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run pulsar/index.ts",
    "build": "bun build ./pulsar/index.ts --outfile ./dist/index.js --target bun --external \"@computer-use/nut-js\" --external \"@ui-tars/operator-nut-js\" --external jimp",
    "start": "bun run ./dist/index.js"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/jimp": "^0.2.28"
  },
  "peerDependencies": {
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.1.18",
    "@ai-sdk/groq": "^1.2.5",
    "@ai-sdk/openai": "^1.1.18",
    "@computer-use/nut-js": "^4.2.0",
    "@elysiajs/static": "^1.2.0",
    "@elysiajs/websocket": "0.2.8",
    "@i-am-bee/acp-sdk": "^0.0.5",
    "@ui-tars/operator-nut-js": "^1.2.0-beta.17",
    "@ui-tars/sdk": "^1.2.0-beta.24",
    "ai": "^4.2.8",
    "beeai-framework": "^0.1.9",
    "elysia": "0.7.30",
    "exceljs": "^4.4.0",
    "jimp": "^1.6.0",
    "replicate": "^1.0.1"
  }
}